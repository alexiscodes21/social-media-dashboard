  "devDependencies": {
    "copyfiles": "^2.4.1",
    "imagemin-cli": "^7.0.0",
    "lite-server": "^2.6.1",
    "onchange": "^7.1.0",
    "parallelshell": "^3.0.2",
    "rimraf": "^5.0.1",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "uglify-js": "^3.17.4"
